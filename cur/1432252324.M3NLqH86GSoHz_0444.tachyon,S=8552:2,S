function openWin () {
  // browser detect loads differently sized window for netscape 3.x
  if (navigator.appName=="Netscape" && parseInt(navigator.appVersion)<=3) {
    popup =
window.open(popupURL,adname,'toolbar=0,location=0,directories=0,status=0,men
